{
  "name": "@breadlee/eslint",
  "version": "0.0.4",
  "description": "leeyc package eslint config",
  "type": "module",
  "keywords": [],
  "author": "leeyc",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
